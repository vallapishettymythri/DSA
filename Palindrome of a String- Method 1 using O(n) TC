#include<iostream>
using namespace std;
int main(){
string s; //intialising s as string data type
getline(cin,s); //getline is used for user input to take string.
string rev=""; //empty string
int n=s.length(); //setting n as length of the string which is in integer
for(int i=n-1;i>=0;i--){ //checking from the n-1 which is from last.
rev+=s[i]; //rev=rev+s[i]
}
if(s==rev){ //if s is equal to rev then print palindrome
cout<<"Palindrome";
}else{
cout << "Not Palindrome"; //else print not palindrome.
}
return 0;
}
